<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/homeFragment">

    <fragment
        android:id="@+id/homeFragment"
        android:name="com.example.thirdweek.HomeFragment"
        android:label="HomeFragment" >
        <action
            android:id="@+id/action_homeFragment_to_authorizationFragment"
            app:destination="@id/authorizationFragment" />
        <action
            android:id="@+id/action_homeFragment_to_authorizationConstraintFragment"
            app:destination="@id/authorizationConstraintFragment" />
        <action
            android:id="@+id/action_homeFragment_to_calculatorFragment"
            app:destination="@id/calculatorFragment" />
        <action
            android:id="@+id/action_homeFragment_to_musicFragment"
            app:destination="@id/musicFragment" />
        <action
            android:id="@+id/action_homeFragment_to_profileFragment"
            app:destination="@id/profileFragment" />
        <action
            android:id="@+id/action_homeFragment_to_authorizationConstraintFragment2"
            app:destination="@id/authorizationConstraintFragment" />
        <action
            android:id="@+id/action_homeFragment_to_calculatorConstraintFragment"
            app:destination="@id/calculatorConstraintFragment" />
        <action
            android:id="@+id/action_homeFragment_to_musicConstraintFragment"
            app:destination="@id/musicConstraintFragment" />
        <action
            android:id="@+id/action_homeFragment_to_profileConstraintFragment"
            app:destination="@id/profileConstraintFragment" />
        <action
            android:id="@+id/action_homeFragment_to_authorizationComposeActivity"
            app:destination="@id/authorizationComposeActivity" />
        <action
            android:id="@+id/action_homeFragment_to_musicComposeActivity"
            app:destination="@id/musicComposeActivity" />
    </fragment>
    <fragment
        android:id="@+id/authorizationFragment"
        android:name="com.example.thirdweek.withoutConstraint.AuthorizationFragment"
        android:label="fragment_authorization"
        tools:layout="@layout/fragment_authorization" />
    <fragment
        android:id="@+id/authorizationConstraintFragment"
        android:name="com.example.thirdweek.constraint.AuthorizationConstraintFragment"
        android:label="fragment_authorization_constraint"
        tools:layout="@layout/fragment_authorization_constraint" />
    <fragment
        android:id="@+id/calculatorFragment"
        android:name="com.example.thirdweek.withoutConstraint.CalculatorFragment"
        android:label="fragment_calculator"
        tools:layout="@layout/fragment_calculator" />
    <fragment
        android:id="@+id/calculatorConstraintFragment"
        android:name="com.example.thirdweek.constraint.CalculatorConstraintFragment"
        android:label="fragment_calculator_constraint"
        tools:layout="@layout/fragment_calculator_constraint" />
    <fragment
        android:id="@+id/musicFragment"
        android:name="com.example.thirdweek.withoutConstraint.MusicFragment"
        android:label="fragment_music"
        tools:layout="@layout/fragment_music" />
    <fragment
        android:id="@+id/musicConstraintFragment"
        android:name="com.example.thirdweek.constraint.MusicConstraintFragment"
        android:label="fragment_music_constraint"
        tools:layout="@layout/fragment_music_constraint" />
    <fragment
        android:id="@+id/profileFragment"
        android:name="com.example.thirdweek.withoutConstraint.ProfileFragment"
        android:label="fragment_profile"
        tools:layout="@layout/fragment_profile" />
    <fragment
        android:id="@+id/profileConstraintFragment"
        android:name="com.example.thirdweek.constraint.ProfileConstraintFragment"
        android:label="fragment_profile_constraint"
        tools:layout="@layout/fragment_profile_constraint" />
    <activity
        android:id="@+id/authorizationComposeActivity"
        android:name="com.example.thirdweek.compose.AuthorizationComposeActivity"
        android:label="AuthorizationComposeActivity" />
    <activity
        android:id="@+id/musicComposeActivity"
        android:name="com.example.thirdweek.compose.MusicComposeActivity"
        android:label="MusicComposeActivity" />
</navigation>